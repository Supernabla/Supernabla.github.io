{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["DOM", "ES2020"],
    "module": "ES2020",
    "moduleResolution": "node",
    "noEmit": true,
    "noUnusedLocals": true,
    "paths": {
      "config/*": ["./.gatsby/*"],
      "components/*": ["src/components/*"],
      "context/*": ["src/context/*"],
      "hooks/*": ["src/hooks/*"],
      "utils/*": ["src/utils/*"],
      "views/*": ["src/views/*"]
    },
    "strict": true,
    "target": "ES2020"
  },
  "exclude": ["node_modules", "public", ".cache"]
}
